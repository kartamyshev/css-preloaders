.btns
  position absolute
  left 50%
  top 20px
  width 150px
  margin-left -(@width/2)
  display flex
  .get-loader
    background-color transparent;
    border 1px solid #f07444;
    color g-color;
    transition all 0.6s ease 0s;
    width 150px
    margin 0 5px
    height 45px
    border-radius 2px
    cursor pointer
    &:hover
      background-color g-color;
      color white;
      transform scale(1.1)
    &:active
      transform scale(.9)
